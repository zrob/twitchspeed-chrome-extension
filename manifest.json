{
    "name": "TwitchSpeed",
    "version": "0.0.1",
    "description": "Toggle 1x and 2x Twitch Playback Speed",
    "manifest_version": 3,
    "commands": {
        "toggle": {
            "suggested_key": "Ctrl+Shift+L",
            "description": "Toggle speed"
        }
    },
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}